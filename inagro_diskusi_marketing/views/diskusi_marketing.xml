<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  

  	<record model="ir.actions.act_window" id="act_window_diskusi_marketing">
        <field name="name">Form Marketing Discuss</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">diskusi.marketing</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="context">{"search_default_requested_by":uid}</field> -->
        <field name="context">{}</field>
        <!-- <field name="search_view_id" ref="view_sprogroup_purchase_request_search"/> -->
        <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            Click to start a new marketing discuss.
          </p>
        </field>
    </record>

    <record model="ir.actions.act_window" id="act_window_diskusi_marketing_room">
        <field name="name">Room</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">room.marketing</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="context">{"search_default_requested_by":uid}</field> -->
        <field name="context">{}</field>
        <!-- <field name="search_view_id" ref="view_sprogroup_purchase_request_search"/> -->
        <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            Click to start a new room.
          </p>
        </field>
    </record>

    <menuitem id="menu_diskusi_marketing"
    name="Marketing Discuss"
    groups='inagro_diskusi_marketing.user_diskusi_marketing'
    web_icon="inagro_diskusi_marketing,static/src/img/menu.jpg"/>

    <menuitem
        id="menu_diskusi_marketing_form"
        name="Form Discuss"
        sequence="1"
        parent="inagro_diskusi_marketing.menu_diskusi_marketing"
        groups='inagro_diskusi_marketing.user_diskusi_marketing'
        action="inagro_diskusi_marketing.act_window_diskusi_marketing"
        />

    <menuitem
        id="menu_diskusi_marketing_room"
        name="Room"
        sequence="1"
        parent="inagro_diskusi_marketing.menu_diskusi_marketing"
        groups='inagro_diskusi_marketing.sales_diskusi_marketing,inagro_diskusi_marketing.ga_diskusi_marketing'
        action="inagro_diskusi_marketing.act_window_diskusi_marketing_room"
        />

   
</odoo>
