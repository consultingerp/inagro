<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_room_reservation_report_calendar" model="ir.ui.view">
        <field name="name">Reservation Calendar</field>
        <field name="model">reservation.report</field>
        <field name="arch" type="xml">
            <calendar string="Room reservation report" date_start="checkin" color="state"
                date_stop="checkout">
                <field name="room_name" />
            </calendar>
        </field>
    </record>
        

</odoo>
